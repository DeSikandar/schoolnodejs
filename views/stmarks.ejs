<div class="container">
    <p>Student Name : <%=student[0].name%></p>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Semester</th>
                <th scope="col">Math</th>
                <th scope="col">Science</th>
                <th scope="col">English</th>
                <th scope="col">Total</th>
                <th scope="col">%</th>

            </tr>
        </thead>
        <tbody>

            <% marks.map((s,index)=>{%>

            <tr>
                <th scope="row"><%=index%></th>
                <td><%=s.semester%></td>
                <td><%=s.math%></td>
                <td><%=s.science%></td>
                <td><%=s.english%></td>
                <td><%=s.math+s.science+s.english%></td>
                <td><%=((s.math+s.science+s.english)/3).toFixed(2)%></td>

            </tr>
            <%})%>


        </tbody>
    </table>
</div>