<form method="POST">

    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Student Name</th>
                <th scope="col">Math</th>
                <th scope="col">Science</th>
                <th scope="col">English</th>
            </tr>
        </thead>
        <tbody>
            <% student.map((s,index)=>{%>

            <tr>
                <th scope="row"><%=index%></th>
                <td><%=s.name%></td>
                <td><input type="hidden" name="marks[<%=s.id%>][semester]" value="<%=s.semeseter%>">
                    <input type="text" name="marks[<%=s.id%>][math]" placeholder="Math"></td>
                <td><input type="text" name="marks[<%=s.id%>][science]" placeholder="Science"></td>
                <td><input type="text" name="marks[<%=s.id%>][english]" placeholder="English"></td>
            </tr>
            <%})%>


        </tbody>
    </table>
    <div class="container">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form>